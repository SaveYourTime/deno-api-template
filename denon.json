{
  "$schema": "https://deno.land/x/denon/schema.json",
  "scripts": {
    "start": {
      "cmd": "deno run app.ts",
      "desc": "run my app.ts file"
    }
  },
  "allow": ["env", "net", "read"],
  "unstable": true,
  "env": {
    "HOST": "localhost",
    "PORT": "3000",
    "ORIGIN": "*",
    "JWT_SECRET_KEY": "denon_key",
    "JWT_EXPIRES_IN": "3600000"
  }
}
